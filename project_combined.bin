  return (
    <nav className="absolute bottom-8 left-0 right-0 bg-card/95 backdrop-blur-lg border-t border-border px-2 py-2">
      <div className="flex justify-around items-center">
        {tabs.map(({ id, icon: Icon, label }) => (
          <button
            key={id}
